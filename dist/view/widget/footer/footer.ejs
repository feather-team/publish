<link rel="stylesheet" href="/static/w_/tasks/tasks.css" type="text/css" /><script src="/static/feather.js"></script><script src="/static/m_/c0c0bb3.js"></script><div class="panel panel-primary" id="tasks-wraper" v-show="show" transition="expand">
    <div class="panel-heading" v-on:click="toggle">任务调度列表</div>
    <div class="panel-body" v-bind:class="{'panel-hidden': hidden}">
        <ul id="tasks" class="list-group">
            <template v-for="task in tasks">
                <li class="list-group-item list-group-item-{{task.className}}">
                    <table>
                        <tr><td>描述：</td><td>{{task.desc}}</td></tr>
                        <tr><td>命令：</td><td>{{task.cmd}} {{task.args.join(' ')}}</td></tr>
                        <tr><td>时间：</td><td>{{task.startTime}} ~ {{task.closeTime}}</td></tr>
                        <tr><td>状态：</td><td>任务{{task.text}}</td></tr>
                        <tr v-show="task.showError"><td>原因：</td><td>{{{task.errorMsg}}}</td></tr>
                        <tr v-show="task.showSuccess"><td>返回：</td><td>{{{task.msg}}}</td></tr>
                    </table>
                </li>
            </template>
        </ul>
    </div>
</div><script>require.async('widget/tasks/tasks.js');</script>

</body>
</html>