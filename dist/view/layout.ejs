<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Feather Quick Build</title>
        <link rel="stylesheet" href="/combo?static/c_/bootstrap/css/bootstrap.css&static/style.css&static/w_/header/header.css&static/w_/tasks/tasks.css" type="text/css" /><script src="/static/feather.js"></script><script src="/static/m_/ec0c555.js"></script></head>
<body>

<div class="container header">
    <div class="row">
        <div class="col-md-6 logo">feather自动编译系统</div>
        <div class="col-md-6">
            <!--BLOCK_START#navs-->
                <ul class="nav nav-pills navs">
                    <li class="active"><a href="/">编译</a></li>
                    <li><a href="/repo">仓库</a></li>
                </ul>
            <!--BLOCK_END-->
        </div>
    </div>
</div>
<!--BLOCK_START#container--><!--BLOCK_END-->
<div class="panel panel-primary" id="tasks-wraper" v-show="show" transition="expand">
    <div class="panel-heading" v-on:click="toggle">任务调度列表</div>
    <div class="panel-body" v-bind:class="{'panel-hidden': hidden}">
        <ul id="tasks" class="list-group">
            <template v-for="task in tasks">
                <li class="list-group-item list-group-item-{{task.className}}">
                    <table>
                        <tr><td>描述：</td><td>{{task.desc}}</td></tr>
                        <tr><td>命令：</td><td>{{task.cmd}} {{task.args.join(' ')}}</td></tr>
                        <tr><td>时间：</td><td>{{task.startTime}} ~ {{task.closeTime}}</td></tr>
                        <tr><td>状态：</td><td>任务{{task.text}}</td></tr>
                        <tr v-show="task.showError"><td>原因：</td><td>{{{task.msg}}}</td></tr>
                        <tr v-show="task.showSuccess"><td>返回：</td><td>{{{task.msg}}}</td></tr>
                    </table>
                </li>
            </template>
        </ul>
    </div>
</div><script>require.async('widget/tasks/tasks.js');</script>

</body>
</html>